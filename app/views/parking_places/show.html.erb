<%- model_class = ParkingPlace -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @parking_place.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hourly_rate) %>:</strong></dt>
  <dd><%= @parking_place.hourly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:daily_rate) %>:</strong></dt>
  <dd><%= @parking_place.daily_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:weekly_rate) %>:</strong></dt>
  <dd><%= @parking_place.weekly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:monthly_rate) %>:</strong></dt>
  <dd><%= @parking_place.monthly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:spaces_number) %>:</strong></dt>
  <dd><%= @parking_place.spaces_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @parking_place.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:georeference_x) %>:</strong></dt>
  <dd><%= @parking_place.georeference_x %></dd>
  <dt><strong><%= model_class.human_attribute_name(:georeference_y) %>:</strong></dt>
  <dd><%= @parking_place.georeference_y %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parking_type_id) %>:</strong></dt>
  <dd><%= @parking_place.parking_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:owner_type_id) %>:</strong></dt>
  <dd><%= @parking_place.owner_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:organization_id) %>:</strong></dt>
  <dd><%= @parking_place.organization_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @parking_place.user_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              parking_places_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_parking_place_path(@parking_place), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              parking_place_path(@parking_place),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
