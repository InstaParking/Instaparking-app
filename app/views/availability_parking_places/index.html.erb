<%- model_class = AvailabilityParkingPlace -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:day_of_week_id) %></th>
      <th><%= model_class.human_attribute_name(:start_date) %></th>
      <th><%= model_class.human_attribute_name(:finish_date) %></th>
      <th><%= model_class.human_attribute_name(:hourly_rate) %></th>
      <th><%= model_class.human_attribute_name(:daily_rate) %></th>
      <th><%= model_class.human_attribute_name(:weekly_rate) %></th>
      <th><%= model_class.human_attribute_name(:monthly_rate) %></th>
      <th><%= model_class.human_attribute_name(:spaces_number) %></th>
      <th><%= model_class.human_attribute_name(:parking_place_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @availability_parking_places.each do |availability_parking_place| %>
      <tr>
        <td><%= link_to availability_parking_place.id, availability_parking_place_path(availability_parking_place) %></td>
        <td><%= availability_parking_place.day_of_week_id %></td>
        <td><%= availability_parking_place.start_date %></td>
        <td><%= availability_parking_place.finish_date %></td>
        <td><%= availability_parking_place.hourly_rate %></td>
        <td><%= availability_parking_place.daily_rate %></td>
        <td><%= availability_parking_place.weekly_rate %></td>
        <td><%= availability_parking_place.monthly_rate %></td>
        <td><%= availability_parking_place.spaces_number %></td>
        <td><%= availability_parking_place.parking_place_id %></td>
        <td><%=l availability_parking_place.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_availability_parking_place_path(availability_parking_place), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      availability_parking_place_path(availability_parking_place),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_availability_parking_place_path,
            :class => 'btn btn-primary' %>
