<%- model_class = AvailabilityParkingPlace -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:day_of_week_id) %>:</strong></dt>
  <dd><%= @availability_parking_place.day_of_week_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_date) %>:</strong></dt>
  <dd><%= @availability_parking_place.start_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:finish_date) %>:</strong></dt>
  <dd><%= @availability_parking_place.finish_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hourly_rate) %>:</strong></dt>
  <dd><%= @availability_parking_place.hourly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:daily_rate) %>:</strong></dt>
  <dd><%= @availability_parking_place.daily_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:weekly_rate) %>:</strong></dt>
  <dd><%= @availability_parking_place.weekly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:monthly_rate) %>:</strong></dt>
  <dd><%= @availability_parking_place.monthly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:spaces_number) %>:</strong></dt>
  <dd><%= @availability_parking_place.spaces_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parking_place_id) %>:</strong></dt>
  <dd><%= @availability_parking_place.parking_place_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              availability_parking_places_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_availability_parking_place_path(@availability_parking_place), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              availability_parking_place_path(@availability_parking_place),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
