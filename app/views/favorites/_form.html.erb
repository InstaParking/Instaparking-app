<%= simple_form_for @favorite, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :user_id %>
  <%= error_span(@favorite[:user_id]) %>
  <%= f.input :parking_place_id %>
  <%= error_span(@favorite[:parking_place_id]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                favorites_path, :class => 'btn btn-default' %>
<% end %>
